class Database:
    def __init__(self):
        self.dict = {}
        self.set_ready = True

    def set_value(self, key, val):
        if self.set_ready:
            self.dict[key] = val

    def get_value(self, key):
        return self.dict.get(key)

    def delete_value(self, key):
        val = self.get_value(key)
        self.set_value(key, None)
        return val

    def __str__(self):
        for key in self.dict.keys():
            return "key: " + key \
                   + "\nvalue: " + self.get_value(key)
